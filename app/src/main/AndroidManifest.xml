<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ipanardian.knotes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sticky_note_text"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ipanardian.knotes.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ipanardian.knotes.ManageNotes"></activity>
        <activity android:name="com.ipanardian.knotes.ViewNotes"></activity>
    </application>

</manifest>